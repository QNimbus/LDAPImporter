{
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"quotes": [
			"error",
			"single"
		],
		"max-len": [
			"error",
			250
		],
		"curly": "error",
		"camelcase": [
			"error",
			{
				"properties": "never"
			}
		],
		"no-trailing-spaces": [
			"error"
		],
		"no-irregular-whitespace": [
			"error"
		]
	},
	"parserOptions": {
		"ecmaVersion": 8
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	}
}